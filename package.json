{
  "name": "website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "deploy": "bash -c 'unset npm_config_prefix && . ~/.nvm/nvm.sh && nvm exec 20 npx sst deploy --stage production'",
    "deploy:dev": "bash -c 'unset npm_config_prefix && . ~/.nvm/nvm.sh && nvm exec 20 npx sst deploy --stage dev'",
    "sst:dev": "sst dev",
    "remove": "bash -c 'unset npm_config_prefix && . ~/.nvm/nvm.sh && nvm exec 20 npx sst remove --stage dev'",
    "remove:prod": "bash -c 'unset npm_config_prefix && . ~/.nvm/nvm.sh && nvm exec 20 npx sst remove --stage production'"
  },
  "dependencies": {
    "@codemirror/lang-cpp": "^6.0.3",
